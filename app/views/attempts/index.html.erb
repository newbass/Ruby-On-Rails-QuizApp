<body style="margin:0px;">

<div style="height:100px;font-size:200%;background-color:green ;width:100%;border-bottom: 1px solid black">
<h1  style="padding-left:50px;padding-top: 40px; color:white">Scores</h1>
</div>

<p id="notice"><%= notice %></p>

<table style="margin-left: 100px;margin-top: 100px;">
  <thead>
    <tr>
      <th>Uid</th>
      <th>Qid</th>
      <th>Success</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attempts.each do |attempt| %>
      <tr>
        <td><%= attempt.uid %></td>
        <td><%= attempt.qid %></td>
        <td><%= attempt.success %></td>
        <td><%= attempt.state %></td>
        <td><%= link_to 'Show', attempt %></td>
        <td><%= link_to 'Edit', edit_attempt_path(attempt) %></td>
        # <td><%= link_to 'Destroy', attempt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<!--%= link_to 'New Attempt', new_attempt_path %-->
<div style="margin-left: 100px">
<%= link_to 'Back', '/admin' %>
</div>
